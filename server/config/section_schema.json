{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": ["title", "links"],
    "additionalProperties": false,
    "properties": {
        "title": {
            "type": "string"
        },
        "disabled": {
            "type": "boolean"
        },
        "links": {
            "type": "array",
            "additionalItems": false,
            "items": {
                "type": "object",
                "required": ["title", "destinationURL"],
                "additionalProperties": false,
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "imageURL": {
                        "type": "string"
                    },
                    "destinationURL": {
                        "type": "string"
                    }
                }
            }
        }
    }
}